    table t_bgp;
    peer table master4;
    import none;
    export all;
}
protocol pipe {
    table t_direct;
    peer table t_bgp;
    import none;
    export all;
}
protocol bgp u_as2 {
    ipv4 {
        table t_bgp;
        import all;
        export where net ~ [10.150.0.0/24];
        next hop self;
    };
    local 10.101.0.150 as 150;
    neighbor 10.101.0.2 as 2;
}
